{
  "env": {
    "node": true,
    "commonjs": true,
    "es2020": true,
    "jest": true
  },
  "extends": ["plugin:prettier/recommended", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "linebreak-style":"off",
    "prettier/prettier": "warn",
    "comma-dangle": ["error", "never"]
  },
  "plugins": ["prettier"]
}
